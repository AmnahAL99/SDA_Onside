

import org.junit.jupiter.api.AfterAll;
import org.junit.jupiter.api.BeforeAll;
import org.junit.jupiter.api.Test;
import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
public class GoogleSearchTest {
private static WebDriver driver;
@BeforeAll
public static void setUp() {
// Set the path for the WebDriver System.setProperty("webdriver.chrome.driver", "path/to/your/chromedriver");
// Initialize the WebDriver driver = new ChromeDriver();
// Go to Google's home page driver.get("http://www.google.com"); }
@Test
public void searchMovies() throws InterruptedException {
// Search for "Green Mile" and print results searchAndPrintResults("Green Mile");
// Search for "Premonition" and print results searchAndPrintResults("Premonition");
// Search for "The Curious Case of Benjamin Button" and print results searchAndPrintResults("The Curious Case of Benjamin Button");
} private void searchAndPrintResults(String query) throws InterruptedException
{ WebElement searchBox = driver.findElement(By.name("q"));
searchBox.clear(); searchBox.sendKeys(query + Keys.ENTER);
// Wait for the results to load Thread.sleep(3000);
// Note: It's better to use WebDriverWait in real tests
// Print the number of results
// Note: The actual selector may vary based on Google's UI updates, adjust accordingly WebElement resultsStat = driver.findElement(By.id("result-stats"));
System.out.println("Results for \"" + query + "\": " + resultsStat.getText()); }
 @AfterAll
 public static void tearDown() {
 // Close the browser if (driver != null) { driver.quit(); } } }
————-

import org.junit.jupiter.api.AfterAll; import org.junit.jupiter.api.BeforeAll; import org.junit.jupiter.api.Test; import org.openqa.selenium.By; import org.openqa.selenium.Keys; import org.openqa.selenium.WebDriver; import org.openqa.selenium.WebElement; import org.openqa.selenium.chrome.ChromeDriver; import static org.junit.jupiter.api.Assertions.assertTrue; public class WebNavigationTest { private static WebDriver driver; @BeforeAll public static void setUp() { // Set the path for the WebDriver System.setProperty("webdriver.chrome.driver", "path/to/your/chromedriver"); // Initialize the WebDriver driver = new ChromeDriver(); } @Test public void navigateAndVerify() throws InterruptedException { // Navigate to the website driver.get("https://testpages.herokuapp.com/styled/index.html"); // Click on Locators - Find By Playground Test Page driver.findElement(By.linkText("Locators - Find By Playground Test Page")).click(); // Print the URL System.out.println("Current URL: " + driver.getCurrentUrl()); // Navigate back driver.navigate().back(); // Print the URL System.out.println("URL after navigating back: " + driver.getCurrentUrl()); // Click on WebDriver Example Page driver.findElement(By.linkText("WebDriver Example Page")).click(); // Print the URL System.out.println("URL of WebDriver Example Page: " + driver.getCurrentUrl()); // Enter value 20 and Enter to "Enter Some Numbers inputBox" WebElement inputBox = driver.findElement(By.id("number")); inputBox.sendKeys("20" + Keys.ENTER); // Wait for the message to be displayed Thread.sleep(2000); // Note: It's better to use WebDriverWait in real tests // Verify ‘two, zero’ message is displayed on page WebElement message = driver.findElement(By.id("numbermessage")); assertTrue(message.getText().contains("two, zero"), "The message 'two, zero' is not displayed."); } @AfterAll public static void tearDown() { // Close the browser if (driver != null) { driver.quit(); } } }
——-
import org.junit.jupiter.api.AfterAll; import org.junit.jupiter.api.BeforeAll; import org.junit.jupiter.api.Test; import org.openqa.selenium.By; import org.openqa.selenium.WebDriver; import org.openqa.selenium.WebElement; import org.openqa.selenium.chrome.ChromeDriver; import static org.junit.jupiter.api.Assertions.assertFalse; import static org.junit.jupiter.api.Assertions.assertTrue; public class HerokuLoginTest { private static WebDriver driver; @BeforeAll public static void setUp() { // Set the path for the WebDriver System.setProperty("webdriver.chrome.driver", "path/to/your/chromedriver"); // Initialize the WebDriver driver = new ChromeDriver(); } @Test public void loginTest() throws InterruptedException { // Go to the login page driver.get("https://id.heroku.com/login"); // Enter an email address WebElement emailInput = driver.findElement(By.id("email")); emailInput.sendKeys("example@example.com"); // Replace with a test email // Enter a password WebElement passwordInput = driver.findElement(By.id("password")); passwordInput.sendKeys("password"); // Replace with a test password // Click on the login button WebElement loginButton = driver.findElement(By.name("commit")); loginButton.click(); // Wait for the response Thread.sleep(2000); // Note: It's better to use WebDriverWait in real tests // Check if the error message is visible boolean isErrorVisible = driver.findElements(By.xpath("//*[contains(text(), 'There was a problem with your login')]")).size() > 0; if (isErrorVisible) { System.out.println("Registration Failed"); } else { System.out.println("Registered"); } } @AfterAll public static void tearDown() { // Close the browser if (driver != null) { driver.quit(); } } }
———
import org.junit.jupiter.api.AfterAll; import org.junit.jupiter.api.BeforeAll; import org.junit.jupiter.api.Test; import org.openqa.selenium.By; import org.openqa.selenium.WebDriver; import org.openqa.selenium.WebElement; import org.openqa.selenium.chrome.ChromeDriver; public class SimpleCalculatorTest { private static WebDriver driver; @BeforeAll public static void setUp() { // Set the path for the WebDriver System.setProperty("webdriver.chrome.driver", "path/to/your/chromedriver"); // Initialize the WebDriver driver = new ChromeDriver(); } @Test public void performCalculation() { // Navigate to the Simple Calculator under Micro Apps driver.get("https://testpages.herokuapp.com/styled/index.html"); driver.findElement(By.id("calculatetest")).click(); // Type any number in the first input WebElement firstNumberInput = driver.findElement(By.id("number1")); firstNumberInput.sendKeys("8"); // Example number // Type any number in the second input WebElement secondNumberInput = driver.findElement(By.id("number2")); secondNumberInput.sendKeys("4"); // Example number // Click on Calculate WebElement calculateButton = driver.findElement(By.id("calculate")); calculateButton.click(); // Get the result WebElement result = driver.findElement(By.id("answer")); // Print the result System.out.println("The result is: " + result.getText()); } @AfterAll public static void tearDown() { // Close the browser if (driver != null) { driver.quit(); } } }